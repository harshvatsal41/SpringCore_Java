<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean class="org.spring.AUTOWIRE.Address" name="address">
        <property name="city" value="Delhi"/>
        <property name="houseNo" value="430" />
        <property name="pincode" value="121002" />
    </bean>
    <bean class="org.spring.AUTOWIRE.Person" name="person" autowire="byName">
        <property name="name" value="Harsh" />
        <property name="age" value="21" />
    </bean>
</beans>